<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="MainBrush" Color="#f8f9fa"/>
    <SolidColorBrush x:Key="AdditionalBrush" Color="#dee2e6"/>
    <SolidColorBrush x:Key="TextBrush" Color="#6c757d"/>
    <SolidColorBrush x:Key="AlarmBrush" Color="#a82525"/>
    <Image x:Key="ThemeIcon"
           Height="25"
           Source="..\images\moon-icon.png"/>
    <Image x:Key="BinIcon"
           x:Shared="False"
           Height="25"
           Source="..\images\black-bin-icon.png"/>
    <Image x:Key="CheckIcon"
           x:Shared="False"
           Height="25"
           Source="..\images\black-check-icon.png"/>
    <Image x:Key="UncheckIcon"
           x:Shared="False"
           Height="25"
           Source="..\images\black-uncheck-icon.png"/>
</ResourceDictionary>